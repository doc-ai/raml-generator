  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/mulesoft-labs/raml-generator/issues"
  },
  "homepage": "https://github.com/mulesoft-labs/raml-generator",
  "devDependencies": {
    "blue-tape": "^1.0.0",
    "istanbul": "^0.4.5",
    "tap-spec": "^5.0.0",
    "tslint": "^5.20.1",
    "typescript": "^3.7.4"
  },
  "dependencies": {
    "@types/blue-tape": "^0.1.33",
    "@types/mkdirp": "^0.5.2",
